<html>
<body>
  <h1>Administration</h1>
  <ul>
    {% for section in sections %}
      <li>
        <h2>{{ section.name }}</h2>
        <ul>
          {% for entityMD in section.getEntitiesMetadata() %}
            <li>
              <a href="{{ 'changelist' | adminUrl(section.name, entityMD.name) }}">
                {{ entityMD.name }}
              </a>
            </li>
          {% endfor %}
        </ul>
      </li>
    {% endfor %}
  </ul>
</body>
</html>
